          }
        }
      }
    },
    "demo-lib": {
      "projectType": "library",
      "root": "libs/demo-lib",
      "sourceRoot": "libs/demo-lib/src",
      "prefix": "transloco-files-management",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
